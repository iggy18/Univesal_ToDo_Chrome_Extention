{
    "name": "Getting Started Example",
    "description": "Allows the user to strike/mark-out selected text on any website.",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "./background.js"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting" 
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["styles.css"],
            "js": ["foreground.js"]
        }
    ],
    "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    }
}


