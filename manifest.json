{
    "name": "Getting Started Example",
    "description": "Allows the user to strike/mark-out selected text on any website.",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "./background.js"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting" 
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["styles.css"],
            "js": ["foreground.js"]
        }
    ]
}


